<main class="main about">

 <%= render 'profile_header_and_links' %>

  <div class="container">

    <div class="row bar-div">
      <header class="bar about-bar">
        <h1 class="about-heading about-title">About</h1>
        <%= button_to "Edit Your Profile", about_edit_path, method: :get, :class => "white-text edit pull-right" %>
      </header>
    </div>


    <div class="info-section row">
      <div class="col-xs-6">

        <%= render 'shared/about/leftside' %>

      </div>

      <div class="col-xs-6">

        <%= render 'shared/about/rightside' %>

      </div>

    </div>

  </div>

</main>
